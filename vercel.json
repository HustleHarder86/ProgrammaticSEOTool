{
  "builds": [
    {
      "src": "api/minimal.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/minimal.py"
    }
  ],
  "installCommand": "pip install -r requirements-minimal.txt",
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}